{
    "name": "@getalecs/n8n-nodes-braze",
    "version": "0.1.1",
    "description": "n8n nodes for Braze services including Campaigns, Canvas, Message Sending, Email Templates, Content Blocks, Segments, and Analytics",
    "keywords": [
        "n8n-community-node-package",
        "braze",
        "campaigns",
        "canvas",
        "message-sending",
        "email-templates",
        "content-blocks",
        "segments",
        "analytics",
        "crm"
    ],
    "homepage": "https://www.npmjs.com/package/@getalecs/n8n-nodes-braze",
    "bugs": {
        "url": "https://github.com/dsfaccini/braze-nodes/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dsfaccini/braze-nodes.git"
    },
    "license": "MIT",
    "author": {
        "name": "David Sanchez"
    },
    "type": "commonjs",
    "main": "index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npx rimraf dist && tsc && gulp build:icons",
        "dev": "tsc --watch",
        "format": "prettier nodes credentials --write",
        "lint": "eslint nodes credentials package.json",
        "lintfix": "eslint nodes credentials package.json --fix",
        "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
    },
    "dependencies": {},
    "devDependencies": {
        "@types/node": "^20.19.14",
        "@typescript-eslint/parser": "~8.32.1",
        "eslint": "^8.57.1",
        "eslint-plugin-n8n-nodes-base": "^1.16.3",
        "gulp": "^5.0.1",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "^1.82.0"
    },
    "engines": {
        "node": ">=20.15"
    },
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "credentials/BrazeApi.credentials.js"
        ],
        "nodes": [
            "nodes/BrazeAnalytics/BrazeAnalytics.node.js",
            "nodes/BrazeCampaigns/BrazeCampaigns.node.js",
            "nodes/BrazeCanvas/BrazeCanvas.node.js",
            "nodes/BrazeContentBlocks/BrazeContentBlocks.node.js",
            "nodes/BrazeEmailTemplate/BrazeEmailTemplate.node.js",
            "nodes/BrazeSegments/BrazeSegments.node.js",
            "nodes/BrazeSendMessage/BrazeSendMessage.node.js"
        ]
    }
}
